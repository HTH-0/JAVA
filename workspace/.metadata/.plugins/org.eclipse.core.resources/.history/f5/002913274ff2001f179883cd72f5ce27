package Ch01;

public class C01SystemOut {
	public static void main(String[] args) {
		
		// System.out.print()
		// \n : 개행, 줄바꿈
		// \b : 백스페이스
		// \t : 탭길이(기본 8칸) 만큼 커서 이동
		System.out.print("HELLOWORLD\n");
		System.out.print("HELLOWORLD\n");
		System.out.print("HELLOWORLD\n");
		System.out.print("HELLOWORLD\n");

		// System.out.printf()
		System.out.printf("");		
		System.out.println();
		
		
		
		
	}
}
